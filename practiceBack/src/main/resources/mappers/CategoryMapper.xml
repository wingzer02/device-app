<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.practiceBack.mapper.CategoryMapper">

    <select id="findAll" resultType="com.example.practiceBack.dto.Category">
        SELECT
        cat_id,
        cat_name
        FROM category
        ORDER BY cat_id
    </select>

</mapper>
